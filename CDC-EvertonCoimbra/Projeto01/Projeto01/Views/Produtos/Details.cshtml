@model Modelo.Cadastros.Produto
@{
    ViewBag.Title = "Detalhes Produto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="panel panel-primary">
    <div class="panel-body">
        @Html.Partial("~/Views/Produtos/_PartialDetailsContentPanel.cshtml", Model)
    </div>
    <div class="panel-footer panel-info">
        <div class="col-md-offset-2	col-md-10">
            @Html.ActionLink("Voltar", "Index", null, new { @class = "btn btn-default" })
            @Html.ActionLink("Alterar", "Edit", new { id = Model.FabricanteID }, new { @class = "btn btn-danger" })
        </div>
        <br />
        <br />
    </div>
</div>